// Try to fetch servers from PulseMCP API first, with fallback to static data
let mcpServers: MCPServer[] = [];
try {
  const apiData = await fetchAllMCPServers();
  if (apiData && apiData.length > 0) {
    mcpServers = transformPulseMCPData(apiData);
  } else {
    mcpServers = staticServers;
  }
} catch (error) {
  console.error('Error with PulseMCP API, using static fallback:', error);
  mcpServers = staticServers;
}

// Helper function to get the first letter of server name for icon
function getServerIcon(name: string): string {
  return name.charAt(0).toUpperCase();
}

// Helper function to format star count
function formatStars(stars?: number): string {
  if (!stars) return '';
  if (stars >= 1000) {
    return `★ ${(stars / 1000).toFixed(1)}k`;
  }
  return `★ ${stars}`;
}

